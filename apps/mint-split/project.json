{
    "name": "mint-split",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/mint-split/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/vite:build",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "outputPath": "dist/apps/mint-split"
            },
            "configurations": {
                "development": {
                    "mode": "development"
                },
                "production": {
                    "mode": "production"
                }
            }
        },
        "serve": {
            "executor": "@nrwl/vite:dev-server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "mint-split:build",
                "proxyConfig": "apps/mint-split/proxy.conf.json"
            },
            "configurations": {
                "development": {
                    "buildTarget": "mint-split:build:development",
                    "hmr": true
                },
                "production": {
                    "buildTarget": "mint-split:build:production",
                    "hmr": false
                }
            }
        },
        "preview": {
            "executor": "@nrwl/vite:preview-server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "mint-split:build"
            },
            "configurations": {
                "development": {
                    "buildTarget": "mint-split:build:development"
                },
                "production": {
                    "buildTarget": "mint-split:build:production"
                }
            }
        },
        "test": {
            "executor": "@nrwl/vite:test",
            "outputs": ["coverage/apps/mint-split"],
            "options": {
                "passWithNoTests": true,
                "reportsDirectory": "../../coverage/apps/mint-split"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/mint-split/**/*.{ts,tsx,js,jsx}"]
            }
        },
        "serve-static": {
            "executor": "@nrwl/web:file-server",
            "options": {
                "buildTarget": "mint-split:build"
            }
        }
    },
    "tags": []
}
